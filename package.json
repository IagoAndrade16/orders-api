{
  "name": "orders-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev": "ts-node-dev -r dotenv/config --inspect --transpile-only --ignore-watch node_modules --respawn src/server/server.ts",
    "script": "ts-node-dev",
    "start": "yarn && yarn build && NODE_ENV=prod node dist/server/server.js",
    "test": "jest",
    "typeorm-dev": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm-dev-revert": "yarn typeorm-dev migration:revert -d src/database/source.ts",
    "typeorm-dev-run": "yarn typeorm-dev migration:run -d src/database/source.ts",
    "typeorm-prod": "NODE_ENV=prod ./node_modules/typeorm/cli.js",
    "typeorm-prod-revert": "yarn typeorm-prod migration:revert -d dist/database/source.js",
    "typeorm-prod-run": "yarn typeorm-prod migration:run -d dist/database/source.js"
  },
